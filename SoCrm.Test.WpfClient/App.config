<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_ILoggingService" />
        <binding name="BasicHttpBinding_ICustomerService" />
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost:1065/LoggingService.svc"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ILoggingService"
        contract="Logging.ILoggingService" name="BasicHttpBinding_ILoggingService" />
      <endpoint address="http://localhost:4957/CustomerService.svc"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICustomerService"
        contract="Customer.ICustomerService" name="BasicHttpBinding_ICustomerService" />
    </client>
  </system.serviceModel>
</configuration>